    "typescript": "^5.8.3"
  },
  "overrides": {
    "next": "14.2.32",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
